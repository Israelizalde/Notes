@ARTICLE{Lam2004-ky,
  title     = "The fallibility of bone density values and their use in
               archaeological analyses",
  author    = "Lam, Yin M and Pearson, Osbjorn M",
  journal   = "Journal of Taphonomy",
  publisher = "Palaeontological Network Foundation, The",
  volume    =  2,
  number    =  1,
  pages     = "99--116",
  abstract  = "As a proxy measure of resistance to destructive processes, bone
               density has played a central role in the discussion of
               equifinality and faunal representation. Bone density data sets
               have been derived for a diverse range of species, providing
               zooarchaeologists with a framework for assessing the occurrence
               of density-mediated destructive processes. These data sets vary
               tremendously in accuracy. In addition, the representation of the
               density of a bone portion as a single number -while convenient
               for quantitative analyses- obscures by oversimplification the
               many factors that affect the derivation of density values, a
               complicated process subject to multiple potential sources of
               variation and error. Density data sets and methods of
               quantitative analysis must be chosen with care, and the reasons
               for these choices made explicit. At the same time, it must be
               recognized that, despite the amount of attention that it has
               received, density is only one of many variables that affect the
               ability of a bone to restrict destruction.",
  year      =  2004,
  keywords  = "Tafonomía;T:Zooarchaeology;T:tafonomía;T:Densidad ósea"
}

